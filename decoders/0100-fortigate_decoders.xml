-->
<decoder name="fortigate-firewall-v3-fields">
    <parent>fortigate-firewall-v3</parent>
    <regex offset="after_parent">proto=\d+ service=(\S+) \.+ status=(\S+) src=(\S+) \.+ dst=(\S+) </regex>
    <order>protocol,action,srcip,dstip</order>
</decoder>

