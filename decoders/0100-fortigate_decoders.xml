<!-- FortiOS 3.0 via syslog -->
<decoder name="fortigate-firewall-v3">
    <prematch>date=\S+ time=\.+ devname=\S+ device_id=FG\w+ log_id=\d+ type=\S+ subtype=\S+ pri=\S+ vd=</prematch>
    <type>syslog</type>
</decoder>

<!--
