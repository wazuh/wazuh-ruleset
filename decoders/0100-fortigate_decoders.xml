<!-- FortiOS 5.0 via syslog -->
<decoder name="fortigate-firewall-v5">
    <prematch>date=\S+\.+time=\.+devname=\S+\.+devid=\p*FG\S+\.+logid=</prematch>
    <type>syslog</type>
</decoder>

