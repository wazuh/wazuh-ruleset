  <program_name>^unbound</program_name>
</decoder>

<decoder name="unbound-a">
  <parent>unbound</parent>
  <regex> info: (\S+) (\S+). A IN$| info: (\S+) (\S+) AAAA IN$</regex>
  <order>srcip,url</order>
</decoder>
