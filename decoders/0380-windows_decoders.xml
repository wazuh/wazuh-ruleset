</decoder>

<decoder name="Sysmon-EventID#1_new">
    <parent>windows</parent>
    <type>windows</type>
    <prematch>INFORMATION\(1\)\.+Hashes</prematch>
    <regex>Microsoft-Windows-Sysmon/Operational: \S+\((\d+)\)</regex>
    <order>id</order>
</decoder>

<decoder name="Sysmon-EventID#1_new">
