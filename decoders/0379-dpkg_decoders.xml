<decoder name="dpkg-decoder">
    <prematch>^\d\d\d\d-\d\d-\d\d \d\d:\d\d:\d\d </prematch>
    <regex offset="after_prematch">^(\S+\s\S+)\s+(\S+):(\S+)\s+(\S+)|^(\S+)\s+(\S+):(\S+)\s+(\S+)</regex>
    <order>dpkg_status, package, arch, version</order>
</decoder>
