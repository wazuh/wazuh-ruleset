<decoder name="vm-pop3d">
  <program_name>^vm-pop3d</program_name>
</decoder>

<decoder name="vm-pop3d-fail">
  <parent>vm-pop3d</parent>
  <prematch>^User '</prematch>
  <regex offset="after_prematch">^(\S+)' - \w+ auth, </regex>
  <regex>from=(\S+)$</regex>
  <order>user, srcip</order>
</decoder>
